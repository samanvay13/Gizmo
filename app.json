{
  "expo": {
    "name": "Gizmo",
    "slug": "Gizmo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/GizmoLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/GizmoSplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "./src/assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.app.Gizmo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font"
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e7c5c77b-d0dc-4daf-b94e-042fc7e1914a"
      }
    }
  }
}
